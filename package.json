{
  "name": "todomvc-react-redux-ts",
  "version": "1.0.0",
  "description": "TodoMvc with react, redux and typescript",
  "author": "Robin Van den Broeck <robin.broeck@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.3.1",
    "prop-types": "^15.7.2",
    "react": "^15.7.0",
    "react-dom": "^15.7.0",
    "react-redux": "^5.1.2",
    "redux": "^3.7.2",
    "todomvc-app-css": "^2.4.1"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@types/classnames": "^2.3.1",
    "@types/enzyme": "^3.10.10",
    "@types/enzyme-adapter-react-15": "^1.0.6",
    "@types/react": "^15.0.37",
    "@types/react-dom": "^15.5.1",
    "@types/react-redux": "^4.4.46",
    "@types/react-test-renderer": "^15.5.1",
    "@types/sinon": "^2.3.2",
    "@types/tape": "^4.2.30",
    "@types/webpack": "^5.28.0",
    "ava": "^3.15.0",
    "css-loader": "^6.5.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-15": "^1.4.3",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.1",
    "pre-commit": "^1.2.2",
    "prettier": "2.4.1",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1",
    "sinon": "^2.3.8",
    "source-map-loader": "^0.2.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tslint": "^5.5.0",
    "tslint-config-prettier": "^1.1.0",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-react": "^3.0.0",
    "typescript": "^4.5.2",
    "webpack": "^5.64.3",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0"
  },
  "scripts": {
    "test": "ava",
    "build": "tsc",
    "bundle": "webpack",
    "start": "webpack-dev-server --open",
    "clean": "rimraf build/ dist/ node_modules/",
    "precommit": "lint-staged",
    "lint": "tslint src/**/*.ts src/**/*.tsx -c tslint.json --project tsconfig.json"
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "ava": {
    "extensions": [
      "ts",
      "tsx"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
